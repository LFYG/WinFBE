Function frmLogon_btnOK_Click( ByRef sender As wfxButton, ByRef e As EventArgs) As LRESULT
   ' Display the entered logon details via a modal popup dialog
   frmPopup.ShowDialog( frmLogon )
   
   Function = 0
End Function


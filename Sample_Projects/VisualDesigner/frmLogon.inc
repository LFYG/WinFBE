Function frmLogon_Load( ByRef sender As wfxForm, ByRef e As EventArgs) As LRESULT

   ' Load some items into the listbox
   ' Note: Do not rely on the current ListBox syntaxes because it will most likely change soon.
   dim as long idx
   idx = frmLogon.lstAccess.Items.Add("System Administrator")
   frmLogon.lstAccess.Items.Add("Super User")
   frmLogon.lstAccess.Items.Add("Regular User")
   frmLogon.lstAccess.Items.Add("Guest")
    
   frmLogon.lstAccess.SelectedItem = frmLogon.lstAccess.Item(idx)

   Function = 0
End Function

